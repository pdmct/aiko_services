{
  "version": 0,
  "name":    "p_example",
  "runtime": "python",

  "graph": ["(PE_Add_2)"],

  "parameters": { "constant": 1,
                  "delay":    0.0,
                  "limit":    2,
                  "rate":     1.0
  },

  "elements": [
    { "name":   "PE_Add",
      "parameters": { "tool": "true",
                      "description": "Add a constant to the input"},
      "input":  [{ "name": "i", "type": "int" }],
      "output": [{ "name": "i", "type": "int" }],
      "deploy": {
        "local": { "module": "aiko_services.examples.pipeline.elements"
         }
      }
    },
    { "name":   "PE_Add_2",
      "parameters": { "tool": "true",
                      "description": "Add a two values together"},
      "input":  [{ "name": "i", "type": "int" },
                 { "name": "j", "type": "int" }],
      "output": [{ "name": "k", "type": "int" }],
      "deploy": {
        "local": { "module": "aiko_services.examples.pipeline.elements"
         }
      }
    }
  ]
}
